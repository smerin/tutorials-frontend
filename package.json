{
  "name": "tutorials-frontend",
  "version": "1.0.0",
  "description": "Frontend for tutorials",
  "main": "index.js",
  "scripts": {
    "dev": "next -p 3333",
    "build": "next build",
    "start": "next start",
    "test": "NODE_ENV=test jest --watch",
    "heroku-postbuild": "next build"
  },
  "dependencies": {
    "apollo-boost": "^0.1.21",
    "apollo-client": "^2.4.6",
    "babel-plugin-styled-components": "^1.8.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "next": "^7.0.2",
    "next-with-apollo": "^3.3.0",
    "react": "^16.6.3",
    "react-apollo": "^2.3.1",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.1",
    "styled-normalize": "^8.0.4"
  },
  "devDependencies": {},
  "author": "George Smerin",
  "license": "ISC",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
